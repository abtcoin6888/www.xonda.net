		function loadEmoList(){if("1"==document.frmSearch.calltype.value){var e=serializeGETData("frmSearch"),a=execURL("POST","/xclient/message/emoticon_data.asp",e),n=a.substring(0,4),t=a.substring(5);"E"==n.substring(0,1)?alert("["+n+"] "+t):$("#divEmoticon").html(a),topNaviResize(),bodyResize()}else document.frmSearch.submit();return!1}function searchText(){var e=document.frmSearch;e.EmGubun.value=20,e.page.value=1,loadEmoList(),""!=e.search_text.pval()?$("#btnSearchX").hasClass("display-none")&&$("#btnSearchX").removeClass("display-none"):$("#btnSearchX").hasClass("display-none")||$("#btnSearchX").addClass("display-none")}function searchOk(){var e=document.frmSearch;e.page.value=1,loadEmoList(),""!=e.search_text.pval()?$("#btnSearchX").hasClass("display-none")&&$("#btnSearchX").removeClass("display-none"):$("#btnSearchX").hasClass("display-none")||$("#btnSearchX").addClass("display-none")}function searchCancel(){document.frmSearch.search_text.value="",searchOk()}function changeGubun(e){var a=document.frmSearch;a.EmGubun.value=e,a.EmMonth.value="",a.EmCode.value=0,a.page.value=1,loadEmoList()}function changeMonth(e){var a=document.frmSearch;a.EmMonth.value=e,a.EmCode.value=0,a.page.value=1,loadEmoList()}function changeCate(e){var a=document.frmSearch;a.EmCode.value=e,a.page.value=1,loadEmoList()}function changeSize(e){var a=document.frmSearch;a.pagesize.value=e,a.page.value=1,loadEmoList()}function changePage(e){document.frmSearch.page.value=e,loadEmoList()}function changeHOT(e,a){void 0===a&&(a="");var n=document.frmSearch;n.EmGubun.value="",n.EmMonth.value="",n.EmCode.value=e,n.search_text.value=a,n.page.value=1,searchOk()}

		$(window).ready(function () {
			currentPosition("05");

			$("#txt_search_text").keydown(function(evt) {
				if (evt.keyCode == 13){
					searchText();
					return false;
				}
			});
		});
	</script>
