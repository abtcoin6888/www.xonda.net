<script type="text/javascript">
function downModule(id,cnt,seq,name,post){
	if(id == ""){
		loginLayer("");
	}else if(cnt == 0){
		makeAlert("<b>마이페이지 &gt; 계정관리</b> 메뉴에서 하위계정 추가 후 다운로드 가능합니다.");
	}else if(seq == 0){
		makeAlert("다운로드 가능한 모듈이 없습니다.");
	}else{
		if(post == "DB")
			$("#divDBMS").addClass("has-error");
		else
			$("#divDBMS").removeClass("has-error");

		document.frmModule.seq.value = seq;
		document.frmModule.post.value = post;
		$("#spanModuleName").html(name);
		$("#layerModule").modal("show");
	}
}
function downModuleProc() {
	var frm = document.frmModule;
	if ($("#servern").pval() == "") {
		makeAlert('OS(서버)종류를 입력해주세요.');
		return;
	}else if(frm.post.value == "DB" && $("#dbn").pval() == ""){
		makeAlert('DBMS 종류를 입력해주세요.');
		return;
	} else {
		if($("#dbn").pval() == "") $("#dbn").val(" ");	//IE8때문 (뒷단에서 trim())
		frm.submit();
		$("#layerModule").modal("hide");
	}
}
